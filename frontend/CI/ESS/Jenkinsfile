node('docker') {
       stage('Checkout'){

          checkout scm
       }
       stage('Build Docker'){
            sh 'docker build --file CI/ESS/Dockerfile.test . --tag catanie_test'
       }
       stage('Unit Test Catanie '){
            sh 'docker run  -t catanie_test npm run -- ng test --watch false --single-run true -v ./test:./test '
       }
}