{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "catanie": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "allowedCommonJsDependencies": [
              "deep-equal",
              "file-saver",
              "moment-timezone",
              "rison",
              "shortid",
              "socket.io-client"
            ],
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets/",
              "src/favicon.ico",
              "src/robots.txt",
              "src/manifest.json"
            ],
            "stylePreprocessorOptions": {
              "includePaths": ["src"]
            },
            "styles": ["src/styles.css", "src/theme.scss"],
            "scripts": []
          },
          "configurations": {
            "dmscdev": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "150kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "assets": [
                "src/assets/",
                "src/robots.txt",
                "src/manifest.json",
                {
                  "glob": "site.png",
                  "input": "CI/ESS/",
                  "output": "/assets/images/"
                },
                {
                  "glob": "site-logo.png",
                  "input": "CI/ESS/",
                  "output": "/assets/images/"
                },
                { "glob": "favicon.ico", "input": "CI/ESS/", "output": "/" }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.dmscdev.ts"
                }
              ],
              "stylePreprocessorOptions": {
                "includePaths": ["CI/ESS"]
              },
              "styles": ["src/styles.css", "CI/ESS/theme.scss"],
              "serviceWorker": true
            },
            "e2e": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "150kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "assets": [
                "src/assets/",
                "src/robots.txt",
                "src/manifest.json",
                {
                  "glob": "site.png",
                  "input": "CI/ESS/",
                  "output": "/assets/images/"
                },
                {
                  "glob": "site-logo.png",
                  "input": "CI/ESS/",
                  "output": "/assets/images/"
                },
                { "glob": "favicon.ico", "input": "CI/ESS/", "output": "/" }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.e2e.ts"
                }
              ],
              "stylePreprocessorOptions": {
                "includePaths": ["CI/ESS"]
              },
              "styles": ["src/styles.css", "CI/ESS/theme.scss"],
              "serviceWorker": true
            },
            "rfi-prod": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "150kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "assets": [
                "src/assets/",
                "src/robots.txt",
                "src/manifest.json",
                {
                  "glob": "site.png",
                  "input": "CI/RFI/",
                  "output": "/assets/images/"
                },
                {
                  "glob": "site-logo.png",
                  "input": "CI/RFI/",
                  "output": "/assets/images"
                },
                { "glob": "favicon.ico", "input": "CI/RFI/", "output": "/" }
              ],
              "fileReplacements": [
                {
                  "replace": "src/app/build-specifics/index.ts",
                  "with": "src/app/build-specifics/index.prod.ts"
                },
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.rfi.prod.ts"
                }
              ],
              "stylePreprocessorOptions": {
                "includePaths": ["CI/RFI"]
              },
              "styles": ["src/styles.css", "CI/RFI/theme.scss"]
            },
            "rfi-dev": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "150kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "assets": [
                "src/assets/",
                "src/robots.txt",
                "src/manifest.json",
                {
                  "glob": "site.png",
                  "input": "CI/RFI/",
                  "output": "/assets/images/"
                },
                {
                  "glob": "site-logo.png",
                  "input": "CI/RFI/",
                  "output": "/assets/images/"
                },
                { "glob": "favicon.ico", "input": "CI/RFI/", "output": "/" }
              ],
              "fileReplacements": [
                {
                  "replace": "src/app/build-specifics/index.ts",
                  "with": "src/app/build-specifics/index.prod.ts"
                },
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.rfi.dev.ts"
                }
              ],
              "stylePreprocessorOptions": {
                "includePaths": ["CI/RFI"]
              },
              "styles": ["src/styles.css", "CI/RFI/theme.scss"]
            },
            "maxiv-prod": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "150kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "assets": [
                "src/assets/",
                "src/robots.txt",
                "src/manifest.json",
                {
                  "glob": "site.png",
                  "input": "CI/MAXIV/",
                  "output": "/assets/images/"
                },
                {
                  "glob": "site-logo.png",
                  "input": "CI/MAXIV/",
                  "output": "/assets/images/"
                },
                { "glob": "favicon.ico", "input": "CI/MAXIV/", "output": "/" }
              ],
              "fileReplacements": [
                {
                  "replace": "src/app/build-specifics/index.ts",
                  "with": "src/app/build-specifics/index.prod.ts"
                },
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.maxiv.prod.ts"
                }
              ]
            },
            "development": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "150kb"
                }
              ],
              "assets": [
                "src/assets/",
                "src/robots.txt",
                "src/manifest.json",
                {
                  "glob": "site.png",
                  "input": "CI/PSI/",
                  "output": "/assets/images/"
                },
                {
                  "glob": "site-logo.png",
                  "input": "CI/PSI/",
                  "output": "/assets/images/"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.development.ts"
                }
              ],
              "serviceWorker": true
            },
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "150kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "assets": [
                "src/assets/",
                "src/robots.txt",
                "src/manifest.json",
                {
                  "glob": "site.png",
                  "input": "CI/PSI/",
                  "output": "/assets/images/"
                },
                {
                  "glob": "site-logo.png",
                  "input": "CI/PSI/",
                  "output": "/assets/images/"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/app/build-specifics/index.ts",
                  "with": "src/app/build-specifics/index.prod.ts"
                },
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "staging": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "150kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "assets": [
                "src/assets/",
                "src/robots.txt",
                "src/manifest.json",
                {
                  "glob": "site.png",
                  "input": "CI/PSI/",
                  "output": "/assets/images/"
                },
                {
                  "glob": "site-logo.png",
                  "input": "CI/PSI/",
                  "output": "/assets/images/"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.staging.ts"
                }
              ]
            },
            "qa": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "150kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "assets": [
                "src/assets/",
                "src/robots.txt",
                "src/manifest.json",
                {
                  "glob": "site.png",
                  "input": "CI/PSI/",
                  "output": "/assets/images/"
                },
                {
                  "glob": "site-logo.png",
                  "input": "CI/PSI/",
                  "output": "/assets/images/"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.qa.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "catanie:build",
            "port": 4200,
            "host": "0.0.0.0"
          },
          "configurations": {
            "dmscdev": {
              "browserTarget": "catanie:build:dmscdev"
            },
            "e2e": {
              "browserTarget": "catanie:build:e2e"
            },
            "maxiv-prod": {
              "browserTarget": "catanie:build:maxiv-prod"
            },
            "development": {
              "browserTarget": "catanie:build:development"
            },
            "production": {
              "browserTarget": "catanie:build:production"
            },
            "staging": {
              "browserTarget": "catanie:build:staging"
            },
            "qa": {
              "browserTarget": "catanie:build:qa"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "catanie:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [],
            "styles": ["src/styles.css", "src/theme.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["src"]
            },
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/robots.txt",
              "src/app/loopback_form",
              "src/assets/icons/android-chrome-192x192.png",
              "src/assets/icons/android-chrome-512x512.svg",
              "src/assets/icons/apple-touch-icon-precomposed.png",
              "src/assets/icons/apple-touch-icon.png",
              "src/assets/icons/favicon-16x16.png",
              "src/assets/icons/favicon-32x32.png",
              "src/assets/icons/mstile-70x70.png",
              "src/assets/icons/mstile-150x150.png",
              "src/assets/icons/mstile-310x150.png",
              "src/assets/icons/mstile-310x310.png",
              "src/assets/icons/safari-pinned-tab.svg",
              "src/manifest.json"
            ],
            "codeCoverage": true,
            "codeCoverageExclude": [
              "src/app/shared/sdk/**",
              "src/environments/**",
              "src/polyfills.ts",
              "src/test.ts"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["src/tsconfig.app.json", "src/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "catanie-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "catanie:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "defaultProject": "catanie",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  },
  "cli": {
    "defaultCollection": "@ngrx/schematics"
  }
}
