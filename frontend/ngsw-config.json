{
  "index": "/index.html",
  "dataGroups": [
    {
      "name": "api",
      "urls": [
        "/api",
        "/explorer",
        "/scichatexplorer",
        "/scicat",
        "/panosc",
        "/auth",
        "/_matrix",
        "/openaire",
        "/plexus",
        "/scichat",
        "/riot"
      ],
      "cacheConfig": {
        "maxAge": "0u",
        "maxSize": "0",
        "strategy": "freshness"
      }
    }
  ],
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.html", "/*.css", "/*.js"]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ]
}
